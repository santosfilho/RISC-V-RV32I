module datapath_vlg_sample_tst(
	clock,
	enable_pc,
	load_pc,
	r_rs1,
	r_rs2,
	reset,
	w_rd,
	sampler_tx
);
input  clock;
input  enable_pc;
input  load_pc;
input  r_rs1;
input  r_rs2;
input  reset;
input  w_rd;
output sampler_tx;

reg sample;
time current_time;
always @(clock or enable_pc or load_pc or r_rs1 or r_rs2 or reset or w_rd)
                                                                                
begin                                                 
 if ($realtime > 0)                                   
 begin                                                
	if ($realtime == 0 || $realtime != current_time)  
	begin									          
		if (sample === 1'bx)                          
			sample = 0;                               
		else                                          
			sample = ~sample;                         
	end										          
	current_time = $realtime;					      
 end                                                  
end                                                   

assign sampler_tx = sample;
endmodule

