module datapath_vlg_vec_tst();
// constants                                           
// general purpose registers
reg clock;
reg enable_pc;
reg load_pc;
reg r_rs1;
reg r_rs2;
reg reset;
reg w_rd;
// wires                                               
wire [31:0] saida_teste;

wire sampler;                             

// assign statements (if any)                          
datapath i1 (
// port map - connection between master ports and signals/registers   
	.clock(clock),
	.enable_pc(enable_pc),
	.load_pc(load_pc),
	.r_rs1(r_rs1),
	.r_rs2(r_rs2),
	.reset(reset),
	.saida_teste(saida_teste),
	.w_rd(w_rd)
);

// clock
always
begin
	clock = 1'b0;
	clock = #5000 1'b1;
	#5000;
end 

// enable_pc
initial
begin
	enable_pc = 1'b1;
end 

// load_pc
initial
begin
	load_pc = 1'b0;
end 

// r_rs1
initial
begin
	r_rs1 = 1'b1;
end 

// r_rs2
initial
begin
	r_rs2 = 1'b1;
end 

// reset
initial
begin
	reset = 1'b0;
end 

// w_rd
initial
begin
	w_rd = 1'b1;
end 

datapath_vlg_sample_tst tb_sample (
	.clock(clock),
	.enable_pc(enable_pc),
	.load_pc(load_pc),
	.r_rs1(r_rs1),
	.r_rs2(r_rs2),
	.reset(reset),
	.w_rd(w_rd),
	.sampler_tx(sampler)
);

datapath_vlg_check_tst tb_out(
	.saida_teste(saida_teste),
	.sampler_rx(sampler)
);
endmodule

